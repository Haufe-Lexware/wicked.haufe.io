{
  "api": {
    "name": "portal-api",
    "upstream_url": "${PORTAL_API_URL}",
    "routes": [
      {
        "paths": [
          "/wicked/v1"
        ],
        "strip_path": true,
        "preserve_host": false
      }
    ]  
  },
  "plugins": [
    {
      "config": {
        "header_name": "Correlation-Id",
        "generator": "uuid"
      },
      "name": "correlation-id"
    }
  ]
}
